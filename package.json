    "unplugin-auto-import": "^0.16.7",
    "vite-plugin-vuetify": "^2.0.1",
    "vitest": "^1.2.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vuetify": "^3.3.20"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@phosphor-icons/vue": "^2.1.6",
    "@pinia/nuxt": "^0.4.11",
    "@typescript-eslint/parser": "^6.7.5",
    "@vue/test-utils": "^2.4.1",
    "axios": "^1.5.1",
    "date-fns": "^2.30.0",
